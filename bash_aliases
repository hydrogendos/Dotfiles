# Folder Shortcuts
alias cdstor="cd /media/tora/Storage"
alias cdprog="cd ~/Prog"

# Dotfiles updates
alias dotfiles='git --git-dir=/media/tora/Storage/Prog/Dotfiles/.git/ --work-tree=/media/tora/Storage/Prog/Dotfiles'
alias dotupdate='git --git-dir=/home/tora/Prog/Dotfiles/.git/ --work-tree=/home/tora/Prog/Dotfiles pull; sh /home/tora/Prog/Dotfiles/make.sh'


